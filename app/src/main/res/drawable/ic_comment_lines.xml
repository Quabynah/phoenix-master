<vector xmlns:android="http://schemas.android.com/apk/res/android"
	android:width="24dp"
	android:height="24dp"
	android:viewportHeight="@integer/comment_lines_add_viewport_height"
	android:viewportWidth="@integer/comment_lines_add_viewport_width">
	
	<path
		android:name="comment_bubble"
		android:fillColor="@color/comment_bubble"
		android:pathData="@string/path_comment_bubble" />
	
	<group
		android:name="@string/comment_lines_add_group"
		android:pivotX="@integer/comment_lines_add_center_x"
		android:pivotY="@integer/comment_lines_add_center_y">
		
		<path
			android:name="@string/comment_lines"
			android:pathData="@string/path_comment_lines"
			android:strokeColor="@color/comment_line_stroke_color"
			android:strokeLineCap="square"
			android:strokeWidth="@integer/comment_line_stroke_width" />
	
	</group>

</vector>
